{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://primate-perfect-haddock.ngrok-free.app/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
    },
    {
      "src": "/chat-widget.js",
      "dest": "https://primate-perfect-haddock.ngrok-free.app/chat-widget.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/app",
      "status": 200,
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "status": 200,
      "dest": "/index.html"
    },
    {
      "src": "/install",
      "status": 200,
      "dest": "/index.html"
    },
    {
      "src": "/chat",
      "status": 200,
      "dest": "/index.html",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
    },
    {
      "src": "/auth/callback",
      "status": 200,
      "dest": "/index.html"
    },
    {
      "src": "/error",
      "status": 200,
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "status": 200,
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}